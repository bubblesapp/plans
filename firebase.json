{
  "projectId": "bubbles-plans",
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "headers": [
    {"source": "/service-worker.js", "headers": [{"key": "Cache-Control", "value": "no-cache"}]}
  ],
  "hosting": {
    "site": "cestquoileplan",
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
